{
  "name": "chat-defensoria",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "server": "node ace serve --watch",
    "client": "node ace mix:watch",
    "build": "yarn client:build && yarn server:build",
    "server:build": "node ace build --production",
    "client:build": "node ace mix:build --production",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "5.9.6",
    "@babel/preset-react": "^7.25.9",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "2.5.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/proxy-addr": "^2.0.3",
    "@types/source-map-support": "^0.5.10",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^9",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "laravel-mix": "^6.0.49",
    "laravel-mix-tailwind": "^0.1.2",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.1",
    "react-refresh": "^0.14.2",
    "sass": "^1.81.1",
    "sass-loader": "8.*",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "~4.6",
    "youch": "^3.3.4",
    "youch-terminal": "^2.2.3"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.8.8",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "6.4.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.1.0",
    "@fusionauth/typescript-client": "^1.54.0",
    "adonis-mix-asset": "^3.0.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "proxy-addr": "^2.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.2",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21"
  }
}
